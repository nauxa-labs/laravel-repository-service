{
  "name": "nauxa-labs/laravel-repository-service",
  "description": "A flexible Repository and Service pattern implementation for Laravel applications",
  "keywords": [
    "laravel",
    "repository",
    "service",
    "pattern",
    "eloquent"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nauxa Labs",
      "email": "hello@nauxa.dev"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/console": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64",
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Nauxa\\RepositoryService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nauxa\\RepositoryService\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff",
    "cs-fix": "vendor/bin/php-cs-fixer fix"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nauxa\\RepositoryService\\RepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}